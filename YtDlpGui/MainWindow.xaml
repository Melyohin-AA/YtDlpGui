<Window x:Class="YtDlpGui.MainWindow"
		xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
		xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
		xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
		xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
		xmlns:local="clr-namespace:YtDlpGui"
		mc:Ignorable="d"
		Title="YtDlpGui" Height="300" Width="400">
	<Grid>
		<Grid.RowDefinitions>
			<RowDefinition Height="25"/>
			<RowDefinition/>
		</Grid.RowDefinitions>
		<Menu Grid.Row="0">
			<MenuItem Header="File">
				<MenuItem Header="Download…" Click="DownloadMenuItem_Click"/>
			</MenuItem>
			<MenuItem Header="Help">
				<MenuItem Header="About…" Click="AboutMenuItem_Click"/>
				<MenuItem Header="Check for yt-dlp updates…" Click="CheckForUpdatesMenuItem_Click"/>
			</MenuItem>
		</Menu>
	</Grid>
</Window>
